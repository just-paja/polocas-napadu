{
  "name": "@polocas/ui",
  "version": "0.9.18",
  "description": "UI components for Poločas nápadu interfaces",
  "author": "Pavel Žák <pavel@zak.global>",
  "homepage": "https://polocas-napadu.cz",
  "license": "ISC",
  "main": "dist/index.js",
  "type": "module",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/just-paja/polocas-napadu.git"
  },
  "scripts": {
    "build": "node scripts/build.mjs"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": {
        "development": "./index.tsx",
        "default": "./dist/index.js"
      }
    },
    "./apollo": {
      "types": "./dist/apollo.d.ts",
      "import": "./dist/apollo.js"
    },
    "./buttons": {
      "types": "./dist/buttons.d.ts",
      "import": "./dist/buttons.js"
    },
    "./content": {
      "types": "./dist/content.d.ts",
      "import": {
        "default": "./dist/content.js",
        "development": "./content.tsx"
      }
    },
    "./decorators": {
      "types": "./dist/decorators.d.ts",
      "import": "./dist/decorators.js"
    },
    "./dialogs": {
      "types": "./dist/dialogs.d.ts",
      "import": "./dist/dialogs.js"
    },
    "./errors": {
      "types": "./dist/errors.d.ts",
      "import": "./dist/errors.js"
    },
    "./events": {
      "types": "./dist/events.d.ts",
      "import": "./dist/events.js"
    },
    "./forms": {
      "types": "./dist/forms.d.ts",
      "import": "./dist/forms.js"
    },
    "./inputs": {
      "types": "./dist/inputs.d.ts",
      "import": "./dist/inputs.js"
    },
    "./icons": {
      "types": "./dist/icons.d.ts",
      "import": "./dist/icons.js"
    },
    "./i18n": {
      "types": "./dist/i18n.d.ts",
      "import": "./dist/i18n.js"
    },
    "./locales/": {
      "import": "./locales/",
      "default": "./locales/"
    },
    "./global.scss": "./global.scss",
    "./variables.scss": "./variables.scss",
    "./shows": {
      "types": "./dist/shows.d.ts",
      "import": "./dist/shows.js"
    },
    "./text": {
      "types": "./dist/text.d.ts",
      "import": {
        "default": "./dist/text.js",
        "development": "./text.tsx"
      }
    }
  },
  "dependencies": {
    "@apollo/client": "^3.8.6",
    "@polocas-napadu/core": "file:../core",
    "bootstrap": "^5.2.2",
    "classnames": "^2.3.2",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.43",
    "query-string": "^8.1.0",
    "react": "^18.2.0",
    "react-bootstrap": "^2.9.1",
    "react-hook-form": "^7.47.0",
    "react-i18next": "^12.2.2",
    "react-icons": "^4.11.0",
    "react-overlays": "^5.2.1"
  }
}
